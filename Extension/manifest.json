{
  "manifest_version": 1,

  "name": "Easy Apply",
  "description": "Chrome Extension for filling up job applications",
  "version": "1.0",

  "background" : {
    "scripts" : ["./background.js"]
  },

  "options_page": "./options.html",

  "browser_action": {
    "default_icon": "favicon.png",
	  "default_title": "Easy Apply!"
  },
    
  "permissions": [
    "storage",
    "tabs",
    "http://*/*","https://*/*"
  ]
}

